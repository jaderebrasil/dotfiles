{
"languageserver": {
	"haskell": {
		"command": "haskell-language-server-wrapper",
		"args": ["--lsp"],
		"rootPatterns": [
		  "*.cabal",
		  "stack.yaml",
		  "cabal.project",
		  "package.yaml"
		],
		"filetypes": [
		  "hs",
		  "lhs",
		  "haskell"
		],
		"initializationOptions": {
		  "languageServerHaskell": {
		  }
		}
	},

	"godot": {
	  "host": "127.0.0.1",
	  "filetypes": ["gd", "gdscript3"],
	  "port": 6008
	},

